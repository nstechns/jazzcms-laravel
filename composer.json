{
    "name": "nstechns/jazzcms-laravel",
    "description": "Laravel Jazz sms api or Campaign Management Solution",
    "keywords": [
        "Jazz-SMS-Api",
        "laravel-jazz-sms-api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Naveed Shahzad",
            "email": "nstech42@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^7.1.3|^7.2.5|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "NsTechNs\\JazzCMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NsTechNs\\JazzCMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
    "laravel": {
        "providers": [
            "NsTechNs\\JazzCMS\\JazzCMSServiceProvider"
        ],
        "aliases": {
            "JazzCMS": "NsTechNs\\JazzCMS\\JazzCMSFacade"
        }
    }
}
}
